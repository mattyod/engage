#!/usr/bin/env node

var config = require('../config'),
    make = require('./make')
    log = require('col'),
    app = require('../node_modules/whatApp/whatApp');

module.exports = (function () {
    console.log(app);
    var args = process.argv.slice(2),
        name,
        conf;

    name = args.length ? args[0] : "mfbg";

    if (args.length) {
        make(config.get(name));
    } else {
        log.error('what?');
    }

})();