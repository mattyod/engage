#!/usr/bin/env node

var config = require('../config'),
    make = require('./make')
    log = require('col');

module.exports = (function () {

    var args = process.argv.slice(2),
        name;

    name = args.length ? args[0] : false;

    if (name) {
        make(name, config.get());
    } else {
        log.error('Project name needed');
    }

})();